FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alpha_a;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    top
    {
//        type            zeroGradient;
type fixedValue;
value uniform 0;
 }
    bottom
    {
        type            zeroGradient;
    }
    backPlane
    {
        type            empty;
    }
    frontPlane
    {
        type            empty;
    }
}


