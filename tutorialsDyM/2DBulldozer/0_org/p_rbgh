FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
  #includeEtc "caseDicts/setConstraintTypes"

  "(walls|hole)"
  {
      type            zeroGradient;
  }

  top
  {
   type            fixedValue;
      value           uniform 0;


  }
  bottom
  {
    type            fixedFluxPressure;
      gradient        $internalField;
  }
  inlet
  {
    type            fixedFluxPressure;
      gradient        $internalField;    }
  outlet
  {
    type            fixedFluxPressure;
      gradient        $internalField;    }
  overset
  {
      type            overset;
  }
  frontAndBack
  {
      type            empty;
  }
}

// ************************************************************************* //
